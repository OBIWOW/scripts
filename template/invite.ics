BEGIN:VCALENDAR
VERSION:2.0
PRODID:-//Python 3.10.2//icalendar-5.0.2
CALSCALE:GREGORIAN
BEGIN:VTIMEZONE
TZID:Europe/Oslo
X-LIC-LOCATION:Europe/Oslo
BEGIN:DAYLIGHT
TZNAME:CEST
TZOFFSETFROM:+0100
TZOFFSETTO:+0200
DTSTART:19700329T020000
RRULE:FREQ=YEARLY;BYMONTH=3;BYDAY=-1SU
END:DAYLIGHT
BEGIN:STANDARD
TZNAME:CET
TZOFFSETFROM:+0200
TZOFFSETTO:+0100
DTSTART:19701025T030000
RRULE:FREQ=YEARLY;BYMONTH=10;BYDAY=-1SU
END:STANDARD
END:VTIMEZONE
BEGIN:VEVENT
DTSTART;TZID=Europe/Oslo:${ical_start}
DTEND;TZID=Europe/Oslo:${ical_end}
SUMMARY:${workshop_title}:\n${event_name}
DESCRIPTION:${'Break from 12:00 to 13:00\\n' if workshop_duration == 'all day' else ''}${'How to get to the room:\\n' + room_url if str(room_name) != 'nan' else ''}
LOCATION:${room_name}
END:VEVENT
END:VCALENDAR